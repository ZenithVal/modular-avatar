        <ui:VisualElement name="editContainer" class="container">
            <ui:TextField picking-mode="Ignore" multiline="true" name="noteEditTextField" value="This text will be edited." max-length="8000" class="note-font" style="margin-right: 4px;" />
        </ui:VisualElement>
    </ui:ScrollView>
    <ui:VisualElement name="displayContainerButtons">
        <ui:Button text="Edit" parse-escape-sequences="true" display-tooltip-when-elided="true" name="editButton" />
    </ui:VisualElement>
    <ui:VisualElement name="editContainerButtons">
        <ui:VisualElement style="flex-grow: 0; flex-direction: row;">
            <ui:Button text="Cancel" parse-escape-sequences="true" display-tooltip-when-elided="true" name="cancelButton" class="medium-button" style="flex-grow: 1;" />
            <ui:Button text="Save" parse-escape-sequences="true" display-tooltip-when-elided="true" name="saveButton" class="medium-button" style="flex-grow: 1;" />
        </ui:VisualElement>
    </ui:VisualElement>
